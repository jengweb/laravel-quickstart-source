{
    "name": "quo/quiq-cms-client",
    "description": "QUO-CMS Client",
    "keywords": ["QUO", "cms", "laravel"],
    "license": "MIT",
    "type": "project",
    "author": [
        {
            "name": "Nuttawut Malee",
            "email": "nuttawutmalee@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "approached/laravel-image-optimizer": "^2.3",
        "barryvdh/laravel-cors": "^0.9.2",
        "bkwld/croppa": "~4.0",
        "cknow/laravel-money": "^1.0",
        "emarref/guzzle-param-middleware": "^1.0",
        "fadion/fixerio": "~1.0",
        "guzzlehttp/guzzle": "~6.0",
        "htmlmin/htmlmin": "^4.5",
        "kevinrob/guzzle-cache-middleware": "^2.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "spatie/laravel-partialcache": "^1.2",
        "spatie/laravel-responsecache": "^3.2",
        "torann/geoip": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/CMS/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
